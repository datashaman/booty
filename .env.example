# Required: GitHub webhook secret
WEBHOOK_SECRET=

# Required: Target repository URL
TARGET_REPO_URL=https://github.com/owner/repo

# Required: GitHub token (for cloning private repos and opening PRs)
GITHUB_TOKEN=

# Magentic LLM configuration
MAGENTIC_BACKEND=anthropic
MAGENTIC_ANTHROPIC_API_KEY=
MAGENTIC_ANTHROPIC_MODEL=claude-sonnet-4-5
MAGENTIC_ANTHROPIC_MAX_TOKENS=4096
MAGENTIC_ANTHROPIC_TEMPERATURE=0.0

# Optional: Target branch (default: main)
# TARGET_BRANCH=main

# Optional: Label that triggers the bot (default: agent:builder)
# TRIGGER_LABEL=agent:builder

# Optional: Self-modification
# BOOTY_OWN_REPO_URL=https://github.com/owner/booty
# BOOTY_SELF_MODIFY_ENABLED=true
# BOOTY_SELF_MODIFY_REVIEWER=github-username
